{
    "name": "blitz-php/app-skeleton",
    "description": "Skeleton App for Blitz framework",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Dimitri Sitchet Tomkeu",
            "email": "dimitrisitchet@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "require": {
        "blitz-php/cache": "1.2.2",
        "blitz-php/framework": "^0.10",
        "league/commonmark": "^2.4"
    },
    "require-dev": {
        "filp/whoops": "^2.15"
    },
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "post-update-cmd": [
            "BlitzPHP\\Initializer\\ComposerScripts::postUpdate"
        ]
    }
}
